<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softtechglobal.androidcarmanager">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".add.Reminders" />
        <activity android:name=".add.Notes" />
        <activity android:name=".add.AddReminder" />
        <activity android:name=".add.AddNotes" />
        <activity android:name=".compute.Compute" />
        <activity android:name=".Search" />
        <activity android:name=".statistics.Statistics" />
        <activity android:name=".capture.Capture" />
        <activity android:name=".expenses.AddExpenses" />
        <activity android:name=".usermanagement.Forgotpassword" />
        <activity android:name=".usermanagement.Signup" />
        <activity android:name=".usermanagement.Signin" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>